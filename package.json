{
  "name": "pi_server",
  "version": "1.0.0",
  "description": "Boilerplate for a full-stack MERN app",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "test": "jest",
    "dev:start": "nodemon server/index.js",
    "dev:build": "webpack -w",
    "build": "webpack -p",
    "docker-compose": "docker run -d -p 8080:8080 -v $(pwd):/src/app --name pi_server jlukenoff/pi_server",
    "heroku-postbuild": "webpack -p"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/server/*.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/__test__/coverage/",
    "resolver": null,
    "testMatch": [
      "**/__test__/*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlukenoff/pi_server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jlukenoff/pi_server/issues"
  },
  "homepage": "https://github.com/jlukenoff/pi_server#readme",
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "atob": "^2.1.2",
    "body-parser": "^1.18.3",
    "emotion": "^10.0.7",
    "express": "^4.16.3",
    "jest": "^24.1.0",
    "merge": "^2.1.1",
    "mongoose": "^5.3.0",
    "node-fetch": "^3.1.1",
    "onoff": "^4.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-emotion": "^10.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "supertest": "^3.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-airbnb": "^2.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.0",
    "nodemon": "^1.18.10",
    "style-loader": "^0.23.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.12"
  }
}
